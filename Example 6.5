
;if AL contains 1 or 3 print 'o' & if AL contains 2 or 4 print 'e'

.model small
.stack 100h
.data
.code

main proc
    
    
    
    mov al,3
    
    
    cmp al,'1'
    JE odd       ;Jump equal
    cmp al,'3'
    JE odd
    
    cmp al,'2'
    JE even
    cmp al,'4'
    JE even
    
    odd:
    mov dl,'o'
    JMP Display
    
    even:
    mov dl,'e'
    JMP Display
    
    Display:
    
    mov ah,2
    int 21h
    
    
    
    
main endp
END main
